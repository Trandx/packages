{
  "name": "@trandx/vue-cropper",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "module": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/lib/style.css",
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./css": {
      "import": "./dist/lib/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && npm run cp && npm run mv-pkg-json",
    "preview": "vite preview",
    "cp": "find ./ -maxdepth 1 -type f \\( -name 'pub.*.json' -o -name 'README.md' \\) -exec cp {} ./dist \\; ",
    "mv-pkg-json": "cd ./dist && mv pub.package.json package.json"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "pkgRoot": "dist",
          "access": "public",
          "tag": "latest"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  }
}